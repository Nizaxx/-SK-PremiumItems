options:
	wersja:1.0
	logs:false
on load:
	set {_online} to result of connect to "https://raw.githubusercontent.com/Nizaxx/-SK-PremiumItems/master/version"
	"{@wersja}" is "%{_online}%":
		stop
	{_online} is " " or "":
		stop
	send "&4[PremiumItems]&c DOSTEPNA JEST NOWA WERSJA SKRYPTU! (/preitem update)" to console
	stop

options:
	czas_wydania_po_smierci: 10 seconds	#3 hours
	czas_wydania_po_naprawie: 1 minute	#1 hour
	czas_chowania_preitemu: 1 seconds	#15 seconds
	kapala_ilosc_zabic_potrzebna_do_wskrzeszenia: 30
	
	
	miecz: glowing diamond sword named "&b&lThe Creeper Legend" with lore "&6&oWykuto na nim imie ""%arg-3%""||&c  - 1,5%% szans na mocne odepchniecie przeciwnika||&c  - co 10 obrazenie wzmaga sie||&c   w tobie sila na 6 sekund||&c  - po zabiciu ""diaxa"" otrzymasz dodatkowe||&c   20pkt zycia na minute||&c  - po zabiciu 3 graczy w ciagu 15 sekund||&c   twoj miecz wlada piorunami||&c  - po twojej smierci z tym mieczem kazdemu||&c   wookolo pomiesza sie w glowie po czym splona||&c  - 5%% szans na tryb ucieczki gdy masz||&c   mniej niz 5 serc||&c  - niektore ciosy wbijaja przeciwnika w ziemie||||&9TOTAL: 14 Attack Damage||||&9+7 BONUS Attack Damage"
	luk: bow of infinity 5 named "&2&lRubin Chud" with lore "&6&oImie znalazcy to %arg-3%||&c  - po 15 celnych strzalach uwalnia sie||&c   magiczny pocisk||&c  - po zabiciu 4 graczy w ciagu||&c   30 sekund aktywuje sie furia||&c  - po 5 strzalach w bezruchu||&c   twoje nastepne strzaly zaczna sie palic||&c  - pod komenda /preluk wiadomosc mozesz||&c   wystrzelic wiadomosc tam gdzie opadnie strzala||&c  - nieskonczonnosc strzal, gdy nie masz||&c   strzaly luk Ci 1 dodaje"
	kosa: glowing gold hoe named "&7&lKosa smierci" with lore "&6&o%arg-3% - tak, to ten ktory oszukal ""smierc""||&c  - od 4%% do 6%%  szans na oslepienie przeciwnika||&c  - 1%% szans na rzucenie zaklecia FATUM||&c  - 0,20%% szans na znikniecie na 40 sekund||&c  - po smierci mozesz otrzymac druga szanse||||&9+13,65 Attack Damage"
	miecz_me: glowing diamond sword named "&b&lThe Creeper Legend" with lore "&6&oWykuto na nim imie ""%player%""||&c  - 1,5%% szans na mocne odepchniecie przeciwnika||&c  - co 10 obrazenie wzmaga sie||&c   w tobie sila na 6 sekund||&c  - po zabiciu ""diaxa"" otrzymasz dodatkowe||&c   20pkt zycia na minute||&c  - po zabiciu 3 graczy w ciagu 15 sekund||&c   twoj miecz wlada piorunami||&c  - po twojej smierci z tym mieczem kazdemu||&c   wookolo pomiesza sie w glowie po czym splona||&c  - 5%% szans na tryb ucieczki gdy masz||&c   mniej niz 5 serc||&c  - niektore ciosy wbijaja przeciwnika w ziemie||||&9TOTAL: 14 Attack Damage||||&9+7 BONUS Attack Damage"
	luk_me: bow of infinity 5 named "&2&lRubin Chud" with lore "&6&oImie znalazcy to %player%||&c  - po 15 celnych strzalach uwalnia sie||&c   magiczny pocisk||&c  - po zabiciu 4 graczy w ciagu||&c   30 sekund aktywuje sie furia||&c  - po 5 strzalach w bezruchu||&c   twoje nastepne strzaly zaczna sie palic||&c  - pod komenda /preluk wiadomosc mozesz||&c   wystrzelic wiadomosc tam gdzie opadnie strzala||&c  - nieskonczonnosc strzal, gdy nie masz||&c   strzaly luk Ci 1 dodaje"
	kosa_me: glowing gold hoe named "&7&lKosa smierci" with lore "&6&o%player% - tak, to ten ktory oszukal ""smierc""||&c  - od 4%% do 6%%  szans na oslepienie przeciwnika||&c  - 1%% szans na rzucenie zaklecia FATUM||&c  - 0,20%% szans na znikniecie na 40 sekund||&c  - po smierci mozesz otrzymac druga szanse||||&9+13,65 Attack Damage"
command /premiumitems [<text>] [<text>] [<offline player>]:
	aliases: preitem, premiumitem
	trigger:
		executor is player:
			arg 1 is "clear":
				clear {preitem.do_odbioru.%player%::*}
				clear {preitem.kapala.%player%::*}
			arg 1 is "odbierz":
				{preitem.do_odbioru.%player%::*} is not set:
					send "&4Lista itemow do odbioru jest pusta!"
					stop
				loop {preitem.do_odbioru.%player%::*}:
					"%loop-value%" is "kask_gorniczy":
						player doesn't have space for {preitem.do_odbioru.%player%::kask_gorniczy}:
							send "&4Nie masz miejsca w inwentarzu!"
							stop
						add {preitem.do_odbioru.%player%::kask_gorniczy} to player's inventory
						clear {preitem.do_odbioru.%player%::kask_gorniczy}
						remove loop-value from {preitem.do_odbioru.%player%::*}
						send "&a&l[&4SMS-SHOP&a&l]&2 Odebrano kask gorniczy."
					"%loop-value%" is "kapala":
						player doesn't have space for {preitem.do_odbioru.%player%::kapala}:
							send "&4Nie masz miejsca w inwentarzu!"
							stop
						add {preitem.do_odbioru.%player%::kapala} to player's inventory
						clear {preitem.do_odbioru.%player%::kapala}
						remove loop-value from {preitem.do_odbioru.%player%::*}
						send "&a&l[&4SMS-SHOP&a&l]&2 Odebrano kapale."
				stop
			send "&e&l========&6Premium Items&e&l========"
			send "&6Skrypt ten dodaje itemki o specjalnych wlasciwosciach (m.in. miecz, luk, kapala i wiele innych). Autorem tego skryptu jest Nizax."
			send "&e&l========================"
			stop
		arg 1 is not set:
			send "&4[PremiumItems]&c Komenda nie pelna - /preitem <give;take;update;info>"
			stop
		arg 1 is "update":
			set {_online} to result of connect to "https://raw.githubusercontent.com/Nizaxx/-SK-PremiumItems/master/version"
			"{@wersja}" is "%{_online}%":
				send "&4[PremiumItems]&a Skrypt juz posiada najnowsza wersje!"
				stop
			download "https://raw.githubusercontent.com/Nizaxx/-SK-PremiumItems/master/skript" to "plugins/Skript/scripts/%script%.sk"
			send "&4[PremiumItems]&a Zaktualizowano."
			stop
		arg 1 is "give":
			arg 2 is not set:
				send "&4[PremiumItems]&c Komenda nie pelna - /preitem give <kosa;miecz;luk;repair_points;kask_gorniczy> <nick>"
				stop
			arg 2 is "kosa":
				arg 3 is not set:
					send "&4[PremiumItems]&c Komenda nie pelna - /preitem give kosa <nick>"
					stop
				"%{preitem.magazynek_%arg-3%::*}%" contains "kosa":
					"{@logs}" is "true":
						log "%arg-3% kupil ponownie kose" to "PremiumItems.log"
					set {preitem.magazynek_%arg-3%::kosa::item} to {@kosa}
					set {preitem.magazynek_%arg-3%::kosa::status} to "server"
					send "&a&l[&4SMS-SHOP&a&l]&2 Zakupiono specjalna kose. Wiecej info pod &o/prekosa" to arg-3 
					arg-3 is online:
						remove golden hoe of Infinity 69 from arg-3's inventory
						stop
					add golden hoe of Infinity 69 to {preitem.take_%arg-3%::*}
					stop
				"{@logs}" is "true":
					log "%arg-3% kupil kose" to "PremiumItems.log"
				add "kosa" to {preitem.magazynek_%arg-3%::*}
				add "item" to {preitem.magazynek_%arg-3%::kosa::*}
				add "status" to {preitem.magazynek_%arg-3%::kosa::*}
				add "hitdmg" to {preitem.kosa_%arg-3%::*}
				add "deadloc" to {preitem.kosa_%arg-3%::*}
				add "invisibility" to {preitem.kosa_%arg-3%::*}
				set {preitem.magazynek_%arg-3%::kosa::item} to {@kosa}
				set {preitem.magazynek_%arg-3%::kosa::status} to "server"
				set {preitem.repair_points.%arg-3%} to 0
				send "&a&l[&4SMS-SHOP&a&l]&2 Zakupiono specjalna kose. Wiecej info pod &o/prekosa" to arg-3 
				stop
			arg 2 is "miecz":
				arg 3 is not set:
					send "&4[PremiumItems]&c Komenda nie pelna - /preitem give miecz <nick>"
					stop
				"%{preitem.magazynek_%arg-3%::*}%" contains "miecz":
					"{@logs}" is "true":
						log "%arg-3% kupil ponownie miecz" to "PremiumItems.log"
					set {preitem.magazynek_%arg-3%::miecz::item} to {@miecz}
					set {preitem.magazynek_%arg-3%::miecz::status} to "server"
					send "&a&l[&4SMS-SHOP&a&l]&2 Zakupiono legendarny miecz. Wiecej info pod &o/premiecz" to arg-3
					arg-3 is online:
						remove diamond sword of Infinity 69 from arg-3's inventory
						stop
					add diamond sword of Infinity 69 to {preitem.take_%arg-3%::*}
					stop
				"{@logs}" is "true":
					log "%arg-3% kupil miecz" to "PremiumItems.log"
				add "miecz" to {preitem.magazynek_%arg-3%::*}
				add "item" to {preitem.magazynek_%arg-3%::miecz::*}
				add "status" to {preitem.magazynek_%arg-3%::miecz::*}
				add "victim" to {preitem.miecz_%arg-3%::*}
				add "zabici" to {preitem.miecz_%arg-3%::*}
				add "superdmg" to {preitem.miecz_%arg-3%::*}
				add "diaxvictim" to {preitem.miecz_%arg-3%::*}
				add "ucieczka" to {preitem.miecz_%arg-3%::*}
				add "hit_given" to {preitem.miecz_%arg-3%::*}
				add "hit_taken" to {preitem.miecz_%arg-3%::*}
				set {preitem.magazynek_%arg-3%::miecz::item} to {@miecz}
				set {preitem.magazynek_%arg-3%::miecz::status} to "server"
				set {preitem.repair_points.%arg-3%} to 0
				send "&a&l[&4SMS-SHOP&a&l]&2 Zakupiono legendarny miecz. Wiecej info pod &o/premiecz" to arg-3 
				stop
			arg 2 is "luk":
				arg 3 is not set:
					send "&4[PremiumItems]&c Komenda nie pelna - /preitem give luk <nick>"
					stop
				"%{preitem.magazynek_%arg-3%::*}%" contains "luk":
					"{@logs}" is "true":
						log "%arg-3% kupil ponownie luk" to "PremiumItems.log"
					set {preitem.magazynek_%arg-3%::luk::item} to {@luk}
					set {preitem.magazynek_%arg-3%::luk::status} to "server"
					send "&a&l[&4SMS-SHOP&a&l]&2 Zakupiono jakis luk. Wiecej info pod &o/preluk" to arg-3
					arg-3 is online:
						remove bow of Infinity 5 from arg-3's inventory
						stop
					add bow of Infinity 5 to {preitem.take_%arg-3%::*}
					stop
				"{@logs}" is "true":
					log "%arg-3% kupil luk" to "PremiumItems.log"
				add "luk" to {preitem.magazynek_%arg-3%::*}
				add "item" to {preitem.magazynek_%arg-3%::luk::*}
				add "status" to {preitem.magazynek_%arg-3%::luk::*}
				add "wiadomosc" to {preitem.luk_%arg-3%::*}
				add "lastshoot" to {preitem.luk_%arg-3%::*}
				add "fireball" to {preitem.luk_%arg-3%::*}
				add "ignitearrow" to {preitem.luk_%arg-3%::*}
				add "shootloc" to {preitem.luk_%arg-3%::*}
				add "furia" to {preitem.luk_%arg-3%::*}
				add "furia_mode" to {preitem.luk_%arg-3%::*}
				add "victim" to {preitem.luk_%arg-3%::*}
				set {preitem.magazynek_%arg-3%::luk::item} to {@luk}
				set {preitem.magazynek_%arg-3%::luk::status} to "server"
				set {preitem.repair_points.%arg-3%} to 0
				send "&a&l[&4SMS-SHOP&a&l]&2 Zakupiono jakis luk. Wiecej info pod &o/preluk" to arg-3 
				stop
			arg 2 is "repair_point" or "repair_points":
				arg 3 is not set:
					send "&4[PremiumItems]&c Komenda nie pelna - /preitem give repair_points <nick>"
					stop
				"{@logs}" is "true":
					log "%arg-3% kupil 1 punkt naprawy" to "PremiumItems.log"
				add 1 to {preitem.repair_points.%arg-3%}
				send "&a&l[&4SMS-SHOP&a&l]&2 Zakupiono 1 pkt naprawy. Wiecej info pod &o/preluk;premiecz;prekosa napraw" to arg-3
				stop
			arg 2 is "kask_gorniczy":
				arg 3 is not set:
					send "&4[PremiumItems]&c Komenda nie pelna - /preitem give kask_gorniczy <nick>"
					stop
				"{@logs}" is "true":
					log "%arg-3% kupil kask gorniczy" to "PremiumItems.log"
				add "kask_gorniczy" to {preitem.do_odbioru.%arg-3%::*}
				set {preitem.do_odbioru.%arg-3%::kask_gorniczy} to glowing iron helmet named "&dKask gorniczy"
				send "&a&l[&4SMS-SHOP&a&l]&2 Zakupiono kask gorniczy. Aby go odebrac wpisz &o/preitem odbierz" to arg-3
				stop
			arg 2 is "kapala":
				arg 3 is not set:
					send "&4[PremiumItems]&c Komenda nie pelna - /preitem give kapala <nick>"
					stop
				"{@logs}" is "true":
					log "%arg-3% kupil kapale" to "PremiumItems.log"
				"%{preitem.kapala.%arg-3%::*}%" contains "dead":
					add "kapala" to {preitem.do_odbioru.%arg-3%::*}
					set {preitem.do_odbioru.%arg-3%::kapala} to glowing skeleton head named "&dKapala" with lore "&5Nakarm mnie krwia wrogow, a dam Ci zycie wieczne!||&8UWAGA! Posiadajac kapale bijesz sie slabiej!"
					send "&a&l[&4SMS-SHOP&a&l]&2 Zakupiono kapale. Aby ja odebrac wpisz &o/preitem odbierz" to arg-3
					stop
				add "dead" to {preitem.kapala.%arg-3%::*}
				add "zabici" to {preitem.kapala.%arg-3%::*}
				set {preitem.kapala.%arg-3%::zabici} to 0
				add "wskrzesz" to {preitem.kapala.%arg-3%::*}
				add "victim" to {preitem.kapala.%arg-3%::*}
				add "kapala" to {preitem.do_odbioru.%arg-3%::*}
				set {preitem.do_odbioru.%arg-3%::kapala} to glowing skeleton head named "&dKapala" with lore "&5Nakarm mnie krwia wrogow, a dam Ci zycie wieczne!||&8UWAGA! Posiadajac kapale bijesz sie slabiej!"
				send "&a&l[&4SMS-SHOP&a&l]&2 Zakupiono kapale. Aby ja odebrac wpisz &o/preitem odbierz" to arg-3
				stop
			send "&4[PremiumItems]&c Komenda zle uzupelniona - /preitem give &n%arg-2%?"
			stop
		arg 1 is "take":
			arg 2 is not set:
				send "&4[PremiumItems]&c Komenda nie pelna - /preitem take <kosa;miecz;luk;repair_point> <nick>"
				stop
			arg 2 is "kosa":
				arg 3 is not set:
					send "&4[PremiumItems]&c Komenda nie pelna - /preitem take kosa <nick>"
					stop
				"{@logs}" is "true":
					log "%arg-3% stracil kose (/preitem take)" to "PremiumItems.log"
				clear {preitem.magazynek_%arg-3%::kosa::item}
				clear {preitem.magazynek_%arg-3%::kosa::status}
				remove "item" from {preitem.magazynek_%arg-3%::kosa::*}
				remove "status" from {preitem.magazynek_%arg-3%::kosa::*}
				remove "kosa" from {preitem.magazynek_%arg-3%::*}
				remove "hitdmg" from {preitem.kosa_%arg-3%::*}
				remove "deadloc" from {preitem.kosa_%arg-3%::*}
				remove "invisibility" from {preitem.kosa_%arg-3%::*}
				send "&4[PremiumItems]&a Komenda zostala wykonana pomyslnie."
				arg-3 is online:
					remove golden hoe of Infinity 69 from arg-3's inventory
					stop
				add golden hoe of Infinity 69 to {preitem.take_%arg-3%::*}
				stop
			arg 2 is "miecz":
				arg 3 is not set:
					send "&4[PremiumItems]&c Komenda nie pelna - /preitem take miecz <nick>"
					stop
				"{@logs}" is "true":
					log "%arg-3% stracil miecz (/preitem take)" to "PremiumItems.log"
				clear {preitem.magazynek_%arg-3%::miecz::item}
				clear {preitem.magazynek_%arg-3%::miecz::status}
				remove "item" from {preitem.magazynek_%arg-3%::miecz::*}
				remove "status" from {preitem.magazynek_%arg-3%::miecz::*}
				remove "miecz" from {preitem.magazynek_%arg-3%::*}
				remove "victim" from {preitem.miecz_%arg-3%::*}
				remove "zabici" from {preitem.miecz_%arg-3%::*}
				remove "superdmg" from {preitem.miecz_%arg-3%::*}
				remove "diaxvictim" from {preitem.miecz_%arg-3%::*}
				remove "ucieczka" from {preitem.miecz_%arg-3%::*}
				remove "hit_given" from {preitem.miecz_%arg-3%::*}
				remove "hit_taken" from {preitem.miecz_%arg-3%::*}
				send "&4[PremiumItems]&a Komenda zostala wykonana pomyslnie."
				arg-3 is online:
					remove diamond sword of Infinity 69 from arg-3's inventory
					stop
				add diamond sword of Infinity 69 to {preitem.take_%arg-3%::*}
				stop
			arg 2 is "luk":
				arg 3 is not set:
					send "&4[PremiumItems]&c Komenda nie pelna - /preitem take luk <nick>"
					stop
				"{@logs}" is "true":
					log "%arg-3% stracil luk (/preitem take)" to "PremiumItems.log"
				clear {preitem.magazynek_%arg-3%::luk::item}
				clear {preitem.magazynek_%arg-3%::luk::status}
				remove "item" from {preitem.magazynek_%arg-3%::luk::*}
				remove "status" from {preitem.magazynek_%arg-3%::luk::*}
				remove "luk" from {preitem.magazynek_%arg-3%::*}
				remove "wiadomosc" from {preitem.luk_%arg-3%::*}
				remove "lastshoot" from {preitem.luk_%arg-3%::*}
				remove "fireball" from {preitem.luk_%arg-3%::*}
				remove "ignitearrow" from {preitem.luk_%arg-3%::*}
				remove "shootloc" from {preitem.luk_%arg-3%::*}
				remove "furia" from {preitem.luk_%arg-3%::*}
				remove "furia_mode" from {preitem.luk_%arg-3%::*}
				remove "victim" from {preitem.luk_%arg-3%::*}
				send "&4[PremiumItems]&a Komenda zostala wykonana pomyslnie."
				arg-3 is online:
					remove bow of Infinity 5 from arg-3's inventory
					stop
				add bow of Infinity 5 to {preitem.take_%arg-3%::*}
				stop
			arg 2 is "repair_point" or "repair_points":
				arg 3 is not set:
					send "&4[PremiumItems]&c Komenda nie pelna - /preitem take repair_points <nick>"
					stop
				{preitem.repair_points.%arg-3%} <= 0:
					send "&4[PremiumItems]&c Komenda nie moze zostac wykonana - /preitem take repair_points %arg-3% poniewaz gracz posiada mniej niz 0 pkt napraw"
					stop
				"{@logs}" is "true":
					log "%arg-3% stracil 1 punkt naprawy (/preitem take)" to "PremiumItems.log"
				remove 1 from {preitem.repair_points.%arg-3%}
				send "&4[PremiumItems]&a Komenda zostala wykonana pomyslnie - nowy stan pkt %arg-3% to %{preitem.repair_points.%arg-3%}%"
				stop
			send "&4[PremiumItems]&c Komenda zle uzupelniona - /preitem take &n%arg-2%?"
			stop
		arg 1 is "info":
			send "&e&l========&6Premium Items&e&l========"
			send "&6Skrypt ten dodaje itemki o specjalnych wlasciwosciach (m.in. miecz, luk, kapala i wiele innych). Autorem tego skryptu jest Nizax."
			send "&e&l========================"
			stop
		send "&4[PremiumItems]&c Komenda zle uzupelniona - /preitem &n%arg-1%?"
		stop

command /preluk [<text>] [<text>]:
	trigger:
		"%{preitem.magazynek_%player%::*}%" contains "luk":
			arg 1 is not set:
				send "&e&l========&2&lRubin Chud&e&l========"
				send "&6/preluk wydaj&e dodaje do inwentarza luk"
				send "&6/preluk schowaj&e chowa luk do chmury"
				send "&6/preluk napraw&e naprawia calkowicie luk"
				send "&6/preluk wiadomosc <tresc>&e wysylanie wiadomosci strzala"
				send "&6/preluk info&e informacje o skrypcie"
				send "&e&l======================"
				stop
			arg 1 is "wydaj":
				{preitem.magazynek_%player%::luk::status} is "player":
					send "&4Niestety, mamy w zapasie tylko jeden luk ;\"
					stop
				player doesn't have space for {preitem.magazynek_%player%::luk::item}:
					send "&4Nie masz miejsca w inwentarzu!"
					stop
				loop {preitem.timer4wydaj.%player%::*}:
					{preitem.timer4wydaj.%player%::%loop-value%} is set
					set {_timedelay} to difference between {preitem.timer4wydaj.%player%::%loop-value%} and now
					"%loop-value%" is "smierc_luk":
						{_timedelay} <= {@czas_wydania_po_smierci}
						set {_hasbeenfor} to {@czas_wydania_po_smierci}
						send "&4Od czasu kiedy zgineles rozpoczely sie poszukiwania twojego luku. &cMoze to potrwac nawet {@czas_wydania_po_smierci}!"
						stop
					"%loop-value%" is "naprawa_luk":
						{_timedelay} <= {@czas_wydania_po_naprawie}
						set {_hasbeenfor} to {@czas_wydania_po_naprawie}
						remove {_timedelay} from {_hasbeenfor}
						send "&4Luk bedzie gotowy za %{_hasbeenfor}%."
						stop
					clear {preitem.timer4wydaj.%player%::%loop-value%}
					remove loop-value from {preitem.timer4wydaj.%player%::*}
				player has bow of Infinity 5:
					remove every bow of Infinity 5 from player's inventory
				give {preitem.magazynek_%player%::luk::item} to player
				clear {preitem.magazynek_%player%::luk::item}
				set {preitem.magazynek_%player%::luk::status} to "player"
				send "&eWydano luk."
				stop
			arg 1 is "schowaj":
				player doesn't have bow of Infinity 5:
					send "&4Niestety nie mozesz schowac luku ktorego nie masz."
					stop
				loop all items in player's inventory:
					loop-item is bow of Infinity 5
					{preitem.location_schowaj.%player%} is set:
						send "&4Czekaj, trwa chowanie..."
						stop
					send "&cZaczynasz chowac luk, nie ruszaj sie przez {@czas_chowania_preitemu}."
					set {preitem.location_schowaj.%player%} to block at location of player
					wait {@czas_chowania_preitemu}
					{preitem.location_schowaj.%player%} is block at location of player:
						clear {preitem.location_schowaj.%player%}
						set {preitem.magazynek_%player%::luk::item} to loop-item
						remove loop-item from player's inventory
						set {preitem.magazynek_%player%::luk::status} to "server"
						{preitem.luk_%player%.arrow} is true:
							remove 1 arrow from player's inventory
							clear {preitem.luk_%player%.arrow}
						send "&eSchowano luk."
						stop
					clear {preitem.location_schowaj.%player%}
					send "&4Ruszyles sie, nie zdarzyles schowac luku."
					stop
				send "&4&lBLAD! Zrob SS i zglos to dla administratora!"
				stop
			arg 1 is "napraw":
				{preitem.magazynek_%player%::luk::status} is "player":
					send "&4Schowaj luk do chmury, aby go naprawic &o/preluk schowaj"
					stop
				{preitem.repair_points.%player%} <= 0:
					send "&4Nie mozesz naprawic swojego luku, poniewaz masz %{preitem.repair_points.%player%}% pkt naprawy. Aby takowe punkty zdobyc wystarczy odwiedzic SMS-SHOP na naszym forum."
					stop
				{preitem.timer4wydaj.%player%::smierc_luk} is set:
					send "&4Nie mozna naprawic luku ktorego nie ma w chmurze :\"
					stop
				remove 1 from {preitem.repair_points.%player%}
				set {preitem.magazynek_%player%::luk::item} to {@luk_me}
				set {preitem.magazynek_%player%::luk::status} to "server"
				add "naprawa_luk" to {preitem.timer4wydaj.%player%::*}
				set {preitem.timer4wydaj.%player%::naprawa_luk} to now
				send "&eTwoj luk bedzie gotowy za godzinke."
				stop
			arg 1 is "wiadomosc":
				arg 2 is not set:
					send "&4Wpisz tresc wiadomosci, ktora chcesz przypisac do strzaly."
					stop
				set {preitem.luk_%player%::wiadomosc} to "%arg 2%"
				send "&eTeraz strzel w to miejsce, gdzie ma sie pojawic tekst."
				stop
			arg 1 is "info":
				send "&e&l========&6Premium Items&e&l========"
				send "&6Skrypt ten dodaje itemki o specjalnych wlasciwosciach (m.in. miecz, luk, kapala i wiele innych). Autorem tego skryptu jest Nizax."
				send "&e&l========================"
				stop
			send "&4Niepoprawna komenda."
			stop
		send "&4Niestety nie masz luku ""Rubin Chud"", aby moc uzyc tej komendy. Luk ten juz do kupienia w SMS-SHOP na naszym forum."
		stop
command /premiecz [<text>]:
	trigger:
		"%{preitem.magazynek_%player%::*}%" contains "miecz":
			arg 1 is not set:
				send "&e&l========&b&lThe Creeper Legend&e&l========"
				send "&6/premiecz wydaj&e dodaje do inwentarza miecz"
				send "&6/premiecz schowaj&e chowa miecz do chmury"
				send "&6/premiecz napraw&e naprawia calkowicie miecz"
				send "&6/premiecz info&e informacje o skrypcie"
				send "&e&l======================"
				stop
			arg 1 is "wydaj":
				{preitem.magazynek_%player%::miecz::status} is "player":
					send "&4Niestety, mamy w zapasie tylko jeden miecz ;\"
					stop
				player doesn't have space for {preitem.magazynek_%player%::miecz::item}:
					send "&4Nie masz miejsca w inwentarzu!"
					stop
				loop {preitem.timer4wydaj.%player%::*}:
					{preitem.timer4wydaj.%player%::%loop-value%} is set
					set {_timedelay} to difference between {preitem.timer4wydaj.%player%::%loop-value%} and now
					"%loop-value%" is "smierc_miecz":
						{_timedelay} <= {@czas_wydania_po_smierci}
						set {_hasbeenfor} to {@czas_wydania_po_smierci}
						send "&4Od czasu kiedy zgineles rozpoczely sie poszukiwania twojego miecza. &cMoze to potrwac nawet {@czas_wydania_po_smierci}!"
						stop
					"%loop-value%" is "naprawa_miecz":
						{_timedelay} <= {@czas_wydania_po_naprawie}
						set {_hasbeenfor} to {@czas_wydania_po_naprawie}
						remove {_timedelay} from {_hasbeenfor}
